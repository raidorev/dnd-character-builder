{
  "name": "dnd-character-builder",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "type": "module",
  "scripts": {
    "prepare": "turbo run prepare",
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "test:coverage": "turbo run test:coverage"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@commitlint/prompt-cli": "^17.6.3",
    "eslint-config-custom": "workspace:0.0.0",
    "eslint-formatter-pretty": "^5.0.0",
    "graphql-config": "^5.0.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "stylelint": "^15.6.2",
    "turbo": "^1.9.9",
    "typescript": "^5.0.4"
  },
  "packageManager": "pnpm@7.13.4",
  "engines": {
    "node": ">=16.0.0",
    "pnpm": ">=7.0.0"
  }
}
